{
  "name": "@lppedd/mini-result",
  "version": "0.1.1",
  "license": "MIT",
  "description": "Minimal Result type for TypeScript",
  "author": "Edoardo Luppi <lp.edoardo@gmail.com>",
  "homepage": "https://github.com/lppedd/mini-result",
  "bugs": {
    "url": "https://github.com/lppedd/mini-result/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lppedd/mini-result.git"
  },
  "keywords": [
    "result",
    "minimal",
    "rust",
    "throw",
    "error",
    "handling",
    "functional",
    "typescript",
    "javascript"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.mts",
        "default": "./dist/es/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsc --noEmit && bunchee --sourcemap",
    "test": "npm run build && vitest --pool threads --coverage",
    "lint": "eslint . --fix --cache",
    "lint:ci": "eslint . --max-warnings 0 --cache --cache-location ./node_modules/.cache/eslint/.eslintcache",
    "typedoc": "typedoc src/index.ts --includeVersion"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@types/node": "22.19.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "bunchee": "6.6.2",
    "eslint": "9.39.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "prettier": "3.7.4",
    "typedoc": "0.28.15",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.1",
    "vitest": "3.2.4"
  },
  "sideEffects": false
}
